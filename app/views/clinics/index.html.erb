<!--Made this html as a testing, please erase when creating the view for this action -->
<div class="row justify-content-between pl-3">
  <div class="col-12 col-lg-7">
    <div class="flash flash-success alert alert-dismissible fade show " role="alert">
      <img src="https://res.cloudinary.com/dn5fi6kt7/image/upload/v1614142578/job_doctor_man_szbux6.png" height="80" alt="">
      <span>Cool! we found <strong>21</strong> clinics nearby</span>
      <a data-dismiss="alert" aria-label="Close">
        <i class="fas fa-times"></i>
      </a>
    </div>
    <h2 class="my-3"></h2>
    <div class="cards">
      <%@clinics.each do |clinic|%>

        <%= render 'clinics/card', clinic: clinic %>

     <%end%>

    </div>
  </div>
  <div class="col-12 col-lg-5 mt-3">
    <div id="map"
     style="width: 100%; height: 90vh;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>
  </div>
</div>

